<launch>
	<include file="$(find roguetwo_hardware)/launch/hardware.launch" />
    <include file="$(find roguetwo_control)/launch/control.launch" />
    <include file="$(find roguetwo_navigation)/launch/navigation.launch" />
    <include file="$(find roguetwo_perception)/launch/perception.launch" />
    <node pkg="myahrs_driver" type="myahrs_driver" name="myahrs_driver">
        <param name="port" value="/dev/ttyACM0" />
        <param name="baud_rate" value="115200" />
    </node>

</launch>