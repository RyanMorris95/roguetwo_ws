
<launch>
	<!-- ************ Octomap Server **************** -->
    <node pkg="octomap_server" type="octomap_server_node" name="octomap_server">
            <param name="resolution" value="0.2" />

            <!-- fixed map frame (set to 'map' if SLAM or localization running!) -->
            <param name="frame_id" type="string" value="world" />

            <!-- maximum range to integrate (speedup!) -->
            <param name="sensor_model/max_range" value="10.0" />

            <!-- Ignore the ground plane -->
            <param name="filter_ground" value="False" />

            <!-- Min and max height for cell to be included -->
            <param name="pointcloud_min_z" value="0.35" />
            <param name="pointcloud_max_z" value="1.0" />



    </node> 

    <remap from="svo/pointcloud" to="cloud_in" /> 

    <node pkg="tf" type="static_transform_publisher" name="world_broadcaster" args="0 0 0 0 0 0 world odom 100" />


</launch>