<launch>
  <arg name="cam_name" />
  <arg name="calib_file" default="$(find roguetwo_perception)/svo_files/ps3_mono_calib.yaml"/>

  <node pkg="nodelet" type="nodelet" name="vo_nodelet_manager"  args="manager" output="screen"/>

  <!-- SVO nodelet -->
  <node pkg="nodelet" type="nodelet" name="svo" clear_params="true" args="load svo_ros/SvoNodelet vo_nodelet_manager" output="screen">

    <!-- Camera topic to subscribe to -->
    <param name="cam0_topic" value="/sync/camera_left/image_raw" type="str" />

    <!-- Camera calibration file -->
    <param name="calib_file" value="$(arg calib_file)" />

    <!--Parameters-->
    <rosparam file="$(find roguetwo_perception)/svo_files/pinhole.yaml" />


  </node>
</launch>
